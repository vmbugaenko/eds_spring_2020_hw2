    "def check_url(url):\n",
    "    try:\n",
    "        result = requests.get(url)\n",
    "        result.raise_for_status()\n",
    "        return result.text\n",
    "    except(requests.RequestException, ValueError):\n",
    "        print('–û—à–∏–±–∫–∞!!')\n",
    "        return False"
   ]
