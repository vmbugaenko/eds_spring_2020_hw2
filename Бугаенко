    "def parser(url):\n",
    "    html = check_url(url)\n",
    "    spisok = []\n",
    "    if html is not None:\n",
    "        soup = BeautifulSoup(html, 'html.parser')\n",
    "        tovary = soup.find_all(\"div\", class_=\"description item_table-description\")\n",
    "        for tovar in tovary:\n",
    "            info = {}\n",
    "            url = tovar.find(\"a\", class_=\"snippet-link\")\n",
    "            price = tovar.find(\"div\", class_=\"snippet-price-row\")\n",
    "            metro = tovar.find(\"span\", class_=\"item-address-georeferences-item__content\")\n",
    "            try:\n",
    "                metro = metro.text\n",
